{% extends 'organization/base.html.twig' %}

{% block title %}Création d'un véhicule{% endblock %}

{% block body %}
    <h1>Création d'un véhicule</h1>
    {{ form_start(form) }}
    {{ form_errors(form) }}
    {{ form_row(form.type) }}
    {{ form_row(form.name) }}
    {{ form_row(form.hasMobileRadio) }}
    {{ form_row(form.hasFirstAidKit) }}
    {{ form_row(form.parkingLocation) }}
    {{ form_row(form.contact) }}
    {{ form_row(form.seatingCapacity) }}
    {{ form_end(form) }}
{% endblock %}
